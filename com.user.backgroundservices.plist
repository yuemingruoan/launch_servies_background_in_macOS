<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.user.backgroundservices</string>

    <key>ProgramArguments</key>
    <array>
        <string>__MANAGER_SCRIPT_PATH__</string>
        <string>start</string>
    </array>

    <key>RunAtLoad</key>
    <true/>

    <key>StandardOutPath</key>
    <string>__LOG_PATH__/manager.log</string>

    <key>StandardErrorPath</key>
    <string>__LOG_PATH__/manager.error.log</string>
</dict>
</plist>