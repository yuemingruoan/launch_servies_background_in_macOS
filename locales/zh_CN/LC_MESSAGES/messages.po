msgid "Error: Configuration file not found at {path}"
msgstr "错误: 在 {path} 未找到配置文件"

msgid "Error: Could not decode JSON from {path}"
msgstr "错误: 无法解析 {path} 中的 JSON"

msgid "Error: Service '{service_name}' not found in configuration."
msgstr "错误: 在配置中未找到服务 '{service_name}'"

msgid "Service '{name}' is already running."
msgstr "服务 '{name}' 已在运行中。"

msgid "Successfully launched service '{name}' with PID {pid}."
msgstr "成功启动服务 '{name}'，进程号为 {pid}。"

msgid "Error launching service '{name}': {e}"
msgstr "启动服务 '{name}' 时出错: {e}"

msgid "Service '{service_name}' is not running."
msgstr "服务 '{service_name}' 未在运行。"

msgid "Stopping service '{service_name}' (PID: {pid})..."
msgstr "正在停止服务 '{service_name}' (进程号: {pid})..."

msgid "Process {pid} did not terminate gracefully. Sending SIGKILL."
msgstr "进程 {pid} 未正常终止。正在发送 SIGKILL 信号。"

msgid "Error stopping service '{service_name}': {e}"
msgstr "停止服务 '{service_name}' 时出错: {e}"

msgid "Service '{service_name}' stopped."
msgstr "服务 '{service_name}' 已停止。"

msgid "Starting all enabled services..."
msgstr "正在启动所有已启用的服务..."

msgid "SERVICE NAME"
msgstr "服务名称"

msgid "ENABLED"
msgstr "是否启用"

msgid "STATUS"
msgstr "状态"

msgid "yes"
msgstr "是"

msgid "no"
msgstr "否"

msgid "running"
msgstr "运行中"

msgid "stopped"
msgstr "已停止"

msgid "Service '{service_name}' is already {status_text}."
msgstr "服务 '{service_name}' 已处于 {status_text} 状态。"

msgid "enabled"
msgstr "启用"

msgid "disabled"
msgstr "禁用"

msgid "Service '{service_name}' has been {status_text} in the configuration."
msgstr "服务 '{service_name}' 的配置已被{status_text}。"

msgid "A tool to manage background services on macOS."
msgstr "一个用于管理 macOS 后台服务的工具。"

msgid "Available commands"
msgstr "可用命令"

msgid "Start all enabled services (default action for launchd)."
msgstr "启动所有已启用的服务 (launchd 的默认操作)。"

msgid "Stop a running service (temporary)."
msgstr "停止一个正在运行的服务 (临时)。"

msgid "The name of the service to stop."
msgstr "要停止的服务名称。"

msgid "Restart a service."
msgstr "重启一个服务。"

msgid "The name of the service to restart."
msgstr "要重启的服务名称。"

msgid "Show the status of all configured services."
msgstr "显示所有已配置服务的状态。"

msgid "Enable a service in the configuration file."
msgstr "在配置文件中启用一个服务。"

msgid "The name of the service to enable."
msgstr "要启用的服务名称。"

msgid "Stop and disable a service in the configuration file (permanent)."
msgstr "停止并在配置文件中禁用一个服务 (永久)。"

msgid "The name of the service to disable."
msgstr "要禁用的服务名称。"

msgid "Check the configuration file for syntax and schema errors."
msgstr "检查配置文件是否存在语法和结构错误。"

msgid "Configuration file is valid."
msgstr "配置文件有效。"

msgid "Configuration check failed."
msgstr "配置文件检查失败。"

msgid "Validation Error: {error}"
msgstr "验证错误: {error}"

msgid "Root object must be a dictionary."
msgstr "根对象必须是一个字典。"

msgid "'settings' key is missing or not a dictionary."
msgstr "'settings' 键缺失或不是一个字典。"

msgid "'log_directory' is missing in settings."
msgstr "settings 中缺少 'log_directory'。"

msgid "'services' key is missing or not a list."
msgstr "'services' 键缺失或不是一个列表。"

msgid "Service at index {index} is not a dictionary."
msgstr "索引为 {index} 的服务不是一个字典。"

msgid "Service at index {index} is missing required key: '{key}'."
msgstr "索引为 {index} 的服务缺少必需的键: '{key}'。"

msgid "Service '{name}' has a non-string value for key '{key}'."
msgstr "服务 '{name}' 的键 '{key}' 的值不是一个字符串。"

msgid "Service '{name}' has a non-boolean value for key 'enabled'."
msgstr "服务 '{name}' 的键 'enabled' 的值不是一个布尔值。"

msgid "Service '{name}' has a non-list value for key 'args'."
msgstr "服务 '{name}' 的键 'args' 的值不是一个列表。"

msgid "Service '{name}' has a non-dictionary value for key 'env'."
msgstr "服务 '{name}' 的键 'env' 的值不是一个字典。"

msgid "No services configured. Please edit your 'config.json' to add services."
msgstr "尚未配置任何服务。请编辑 'config.json' 文件以添加服务。"

msgid "It seems you are using the default template. Please edit 'config.json' to configure your own services."
msgstr "您似乎正在使用默认模板。请编辑 'config.json' 文件来配置您自己的服务。"

msgid "Service '{name}' must have either 'command' or 'shell_command', but not both."
msgstr "服务 '{name}' 必须且只能拥有 'command' 或 'shell_command' 中的一个。"

msgid "Service '{name}' has a non-string value for key 'shell_command'."
msgstr "服务 '{name}' 的键 'shell_command' 的值不是一个字符串。"

msgid "ERROR"
msgstr "错误"